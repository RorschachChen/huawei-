arr = [i for i in range(1, 51)]
# print(arr)
start = 1
res = 50
while res>=1:
    tmp = []
    for i in range(start, start+res):
        if '7' not in str(i) and i%7!=0:
            tmp.append(arr[i-start])
    start += res
    res = len(tmp)
    arr = tmp
    if len(arr)==1:
        break
    # print(arr)
print(arr[0])
